# .env file for UrbanCopilot production environment

# Flask configuration
FLASK_ENV=production
FLASK_DEBUG=False
SECRET_KEY=your_secure_secret_key   

# Database configuration (PostgreSQL)
DATABASE_URL=postgresql://user:password@db:5432/urban_copilot

# Azure services configuration
AZURE_API_KEY=your-azure-api-key       # Azure API key for authentication
AZURE_ENDPOINT=your-azure-endpoint     # Azure service endpoint URL

# Application entry point and port configuration
FLASK_APP=main.py                      # Entry point for the Flask app
PORT=5000                              # Application port
